{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Azure": "Error",
      "Microsoft": "Error",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "Information",
        "Azure": "Error",
        "Microsoft": "Error",
        "Microsoft.Hosting.Lifetime": "Information"
      },
      "EnableAdaptiveSampling": false,
      "EnableDependencyTrackingTelemetryModule": false,
      "EnablePerformanceCounterCollectionModule": false
    }
  },
  "AzureOpenAI": {
    "Endpoint": "{{AZURE_OPENAI_ENDPOINT}}",
    "DeploymentName": "{{AZURE_OPENAI_DEPLOYMENT_NAME}}"
  },
  "AzureStorageBlobs": {
    "Endpoint": "{{AZURE_STORAGE_BLOBS_ENDPOINT}}",
    "ContainerName": "{{AZURE_STORAGE_BLOBS_CONTAINER_NAME}}"
  },
  "Connections": {
    "ServiceConnection": {
      "Settings": {
        "AuthType": "ClientSecret",
        "ClientId": "{{MICROSOFT_365_AGENT_ID}}",
        "ClientSecret": "{{MICROSOFT_365_AGENT_PASSWORD}}",
        "TenantId": "{{MICROSOFT_365_AGENT_TENANT_ID}}",
        "Scopes": [
          "https://api.botframework.com/.default"
        ]
      }
    }
  },
  "ConnectionsMap": [
    {
      "ServiceUrl": "*",
      "Connection": "ServiceConnection"
    }
  ],
  "TokenValidation": {
    "Audiences": [
      "{{MICROSOFT_365_AGENT_ID}}"
    ],
    "TenantId": "{{MICROSOFT_TENANT_ID}}"
  }
}
