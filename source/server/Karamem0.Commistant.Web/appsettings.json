{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "Information"
      },
      "EnableDependencyTrackingTelemetryModule": false,
      "EnablePerformanceCounterCollectionModule": false
    }
  },
  "AzureStorageBlobs": {
    "Endpoint": "{{AZURE_STORAGE_BLOB_ENDPOINT}}",
    "ContainerName": "{{AZURE_STORAGE_BLOB_CONTAINER_NAME}}",
    "ClientId": "{{AZURE_STORAGE_BLOB_CLIENT_ID}}"
  },
  "AzureOpenAI": {
    "Endpoint": "{{AZURE_OPENAI_ENDPOINT}}",
    "ModelName": "{{AZURE_OPENAI_MODEL_NAME}}",
    "ClientId": "{{AZURE_OPENAI_CLIENT_ID}}"
  },
  "BotFramework": {
    "MicrosoftAppType": "{{MSAL_APP_TYPE}}",
    "MicrosoftAppId": "{{MSAL_APP_ID}}",
    "MicrosoftAppPassword": "{{MSAL_APP_SECRET}}",
    "MicrosoftAppTenantId": "{{MSAL_TENANT_ID}}"
  }
}
