{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Aspire.Hosting.Dcp": "Warning"
    }
  },
  "Azure": {
    "SubscriptionId": "{{AZURE_SUBSCRIPTION_ID}}",
    "ResourceGroup": "{{AZURE_RESOURCE_GROUP}}",
    "Location": "{{AZURE_LOCATION}}",
    "AllowResourceGroupCreation": false
  },
  "Parameters": {
    "AzureOpenAIName": "{{AZURE_OPENAI_NAME}}",
    "AzureOpenAIResourceGroup": "{{AZURE_OPENAI_RESOURCE_GROUP}}",
    "AzureStorageAccountName": "{{AZURE_STORAGE_ACCOUNT_NAME}}",
    "AzureStorageAccountResourceGroup": "{{AZURE_STORAGE_ACCOUNT_RESOURCE_GROUP}}"
  }
}
