{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "Information"
      },
      "EnableDependencyTrackingTelemetryModule": false,
      "EnablePerformanceCounterCollectionModule": false
    }
  },
  "AzureStorageBlobs": {
    "Endpoint": "{{AZURE_STORAGE_BLOBS_ENDPOINT}}",
    "ContainerName": "{{AZURE_STORAGE_BLOBS_CONTAINER_NAME}}"
  },
  "BotFramework": {
    "MicrosoftAppId": "{{BOT_FRAMEWORK_MICROSOFT_APP_ID}}",
    "MicrosoftAppPassword": "{{BOT_FRAMEWORK_MICROSOFT_APP_PASSWORD}}",
    "MicrosoftAppTenantId": "{{BOT_FRAMEWORK_MICROSOFT_APP_TENANT_ID}}"
  },
  "MicrosoftIdentity": {
    "ClientId": "{{MICROSOFT_APP_ID}}",
    "ClientSecret": "{{MICROSOFT_APP_SECRET}}",
    "Instance": "https://login.microsoftonline.com",
    "TenantId": "{{MICROSOFT_TENANT_ID}}"
  }
}
