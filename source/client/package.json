{
  "name": "@karamem0/commistant",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "start": "tsc && vite",
    "build": "tsc -b && vite build",
    "eslint": "eslint src",
    "eslint:fix": "eslint src --fix",
    "stylelint": "stylelint 'src/**/*.{css,scss,jsx,tsx}'",
    "stylelint:fix": "stylelint 'src/**/*.{css,scss,jsx,tsx}' --fix",
    "formatjs:extract": "formatjs extract 'src/**/messages.ts' --out-file 'src/translations/ja.json' --id-interpolation-pattern '[sha512:contenthash:base64:6]' --format simple",
    "formatjs:compile": "formatjs compile 'src/translations/ja.json' --out-file 'src/translations/compiled/ja.json' --ast --format simple"
  },
  "dependencies": {
    "@automapper/pojos": "^8.8.1",
    "@emotion/react": "^11.14.0",
    "@fluentui/react-components": "^9.72.7",
    "@microsoft/applicationinsights-react-js": "^19.3.8",
    "@microsoft/applicationinsights-web": "^3.3.10",
    "@microsoft/teams-js": "2.47.0",
    "axios": "^1.13.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-error-boundary": "^6.0.0",
    "react-hook-form": "^7.67.0",
    "react-icons": "^5.5.0",
    "react-intl": "^7.1.14",
    "react-router-dom": "^7.9.6",
    "react-use": "^17.6.0",
    "ress": "^5.0.2"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.13.5",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.1",
    "@formatjs/cli": "^6.7.4",
    "@stylistic/eslint-plugin": "^5.6.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^5.1.1",
    "babel-plugin-formatjs": "^10.5.41",
    "eslint": "^9.39.1",
    "eslint-plugin-hooks": "^0.4.3",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-sonarjs": "^3.0.5",
    "globals": "^16.5.0",
    "postcss": "^8.5.6",
    "postcss-styled-syntax": "^0.7.1",
    "stylelint": "^16.26.1",
    "stylelint-config-recess-order": "^7.4.0",
    "stylelint-config-standard-scss": "^16.0.0",
    "stylelint-order": "^7.0.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^7.2.6"
  }
